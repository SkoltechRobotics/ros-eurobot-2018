<?xml version="1.0"?>
<launch>

  <rosparam file="$(find eurobot)/config/field_params.yaml" command="load" ns="field" />
  <rosparam file="$(find eurobot)/config/particle_filter_params.yaml" command="load" ns="main_robot/particle_filter" />
  <rosparam file="$(find eurobot)/config/main_robot_params.yaml" command="load" ns="main_robot" />

  <group ns="main_robot">

    <!-- NODES -->
    <node pkg="eurobot" name="stm_node" type="stm_node.py" output="screen"/>
    <node pkg="urg_node" name="lidar_node" type="urg_node" args="_ip_address:=192.168.0.10" output="screen"/>
    <node pkg="eurobot" name="particle_filter_node" type="particle_filter_node.py" output="screen"/>
    <!--node pkg="eurobot" name="track_regulator" type="track_regulator.py" output="screen"/-->

  </group>
  <node pkg="eurobot" name="read_distance_sensors_data" type="read_distance_sensors_data.py" output="screen"/>

</launch>
